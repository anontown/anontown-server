{
  "name": "anontown-server",
  "version": "1.0.0",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cron": "^1.2.1",
    "crypto-js": "^3.1.9-1",
    "elasticsearch": "^13.3.1",
    "express": "^4.14.0",
    "fs-promise": "^1.0.0",
    "jsonschema": "^1.1.0",
    "jsonwebtoken": "^7.4.1",
    "log4js": "^1.1.0",
    "mongodb": "^2.2.11",
    "power-assert": "^1.4.2",
    "request": "^2.79.0",
    "rxjs": "^5.4.0",
    "source-map-support": "^0.4.6",
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cron": "^1.0.30",
    "@types/crypto-js": "^3.1.33",
    "@types/elasticsearch": "^5.0.16",
    "@types/express": "^4.0.34",
    "@types/fs-promise": "^1.0.1",
    "@types/jest": "^21.1.9",
    "@types/jsonschema": "0.0.4",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/log4js": "0.0.32",
    "@types/mongodb": "^2.1.35",
    "@types/node": "^6.0.51",
    "@types/request": "0.0.39",
    "@types/ws": "0.0.41",
    "intelli-espower-loader": "^1.0.1",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "typescript": "^2.5.2"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc&&node ./dist/scripts/app.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "html",
      "json"
    ]
  }
}